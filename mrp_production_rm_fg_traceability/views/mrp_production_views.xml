<openerp>
<data>
<record id="mrp_production_view_form" model="ir.ui.view">
    <field name="name">mrp.production form</field>
    <field name="model">mrp.production</field>
    <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
    <field name="arch" type="xml">
        <data>
            <xpath expr="//form//field[@name='state']" position="before">
                <button name="open_rm_traceability" type="object" string="RM Traceability"/>
                <button name="open_fg_traceability" type="object" string="FG Traceability"/>
            </xpath>
        </data>
    </field>
</record>

</data>
</openerp>
