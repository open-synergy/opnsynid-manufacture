<openerp>
<data>

<record id="qc_test_view_form" model="ir.ui.view">
    <field name="name">Auto Lock/Unlock Lot</field>
    <field name="model">qc.test</field>
    <field name="inherit_id" ref="quality_control.qc_test_form_view"/>
    <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='active']" position="after">
                <field name="auto_lock_lot"/>
                <field name="auto_unlock_lot"/>
            </xpath>
        </data>
    </field>
</record>

</data>
</openerp>
